        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cm.chat.modules.mapper.ChatMsgMapper">

    <resultMap id="BaseResultMap" type="com.cm.chat.modules.domain.ChatMsg">
